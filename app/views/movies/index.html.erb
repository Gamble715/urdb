<h1>URDB</h1>
<ul>
	
		<% @movies.each do |m| %>
			<dt><%= link_to m.title, movie_path(m.id) %></dt>
			<dd><%= m.description%></dd>
			<iframe src="http://giphy.com/embed/<%= m.gif %>" width="281" height="281" frameBorder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>
		<% end %>
	
</ul>
